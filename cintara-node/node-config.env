# Cintara Node Configuration
# These values will be used for automated setup

# Node Configuration
MONIKER=cintara-unified-node
CHAIN_ID=cintara_11001-1

# Setup Behavior
OVERWRITE_CONFIG=y
AUTO_START=true

# Network Configuration
ENABLE_API=true
ENABLE_GRPC=true
ENABLE_PROMETHEUS=true
MINIMUM_GAS_PRICES=0.25cint

# Logging
LOG_LEVEL=info

# Directories
HOME_DIR=/data/.tmp-cintarad
DATA_DIR=/data

# Keys (used by setup script)
KEY_NAME=key3
KEYRING_BACKEND=os
KEY_ALGORITHM=eth_secp256k1